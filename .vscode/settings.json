{
    "C_Cpp.dimInactiveRegions": false,
    "restructuredtext.confPath": "${workspaceFolder}\\docs",

    "terminal.integrated.profiles.windows": {
        "msys64": {
            "path": "C:\\msys64\\usr\\bin\\bash.exe",
            "env": { 
                "CHERE_INVOKING" : "1" ,
                "MSYSTEM": "MINGW64",
            },
        }
    },
    "restructuredtext.languageServer.disabled": true,

    "files.associations": {
        "*.eb": "python",
        "genozip.h": "c",
        "strings.h": "c",
        "type_traits": "c",
        "typeinfo": "c",
        "piz.h": "c",
        "sam_private.h": "c",
        "errno.h": "c",
        "*.data": "c"
    }
}
