            //"program": "${workspaceFolder}/src/genols-debug.exe",
            
            // "args" : ["--show-codec", "-f", "private/test/test.human2.bam"],
            "args" : ["-f", "bug.bam"],
            // "args" : ["--decompress", "--test", "--echo", "--no-tip", "bug.bam.genozip"],

            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "c:\\msys64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
