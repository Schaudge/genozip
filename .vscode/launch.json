        {
            "name": "Run Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/genozip-debug.exe",
            // "program": "${workspaceFolder}/src/genounzip-debug.exe",
            // "program": "${workspaceFolder}/src/genocat-debug.exe",
            // "program": "${workspaceFolder}/src/genozip-opt.exe",
